@model SSProjectFollowUp.ViewModels.ProjectVM

<div class="row delResult0">
    <div class="form-control">
        <label>User</label>
        <input asp-for="projectsubresultt0.IncharCharge" readonly />
    </div>
    <div class="form-control">
        <label>Result</label>
        <input asp-for="projectsubresultt0.PSRResult" readonly />
    </div>
    <div class="form-control">
        <label>Comment</label>
        <textarea asp-for="projectsubresultt0.PSRComment" readonly></textarea>
    </div>
    <div>
        @for (int i = 0; i < Model.projectFiles.ToList().Count(); i++)
        {
            <a href="~/Documents/@Model.projectFiles.ToList()[i].FUrl@Model.projectFiles.ToList()[i].FExtention">@Model.projectFiles.ToList()[i].FName</a>
        }
    </div>
</div>

